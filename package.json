		"@prisma/client": "^5.9.1",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.2",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/module-alias": "^2.0.4",
		"@types/node": "^20.11.16",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
